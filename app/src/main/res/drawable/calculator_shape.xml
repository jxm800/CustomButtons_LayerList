        android:centerColor="@color/yellowButtonGradient"
        android:startColor="@color/yellowButtonGradient"
        android:endColor="@color/yellowButton"
        android:angle="270"
        android:type="linear" />
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp" />
    <size
        android:width="120dp"
        android:height="70dp" />
    <stroke
        android:width="2dp"
        android:color="@color/yellowButtonDark" />
    <corners android:topRightRadius="0dp" android:bottomRightRadius="0dp"
        android:topLeftRadius="20dp" android:bottomLeftRadius="0dp"/>
</shape>